{
  "name": "kwix",
  "version": "0.0.1",
  "description": "Backend framework for node enthusiasts",
  "author": "k-koehler <kevin.koehler@usask.ca>",
  "homepage": "https://github.com/k-koehler/kwix#readme",
  "license": "MIT",
  "main": "dist/index.js",
